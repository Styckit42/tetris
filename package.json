{
  "name": "red_tetrisboilerplate",
  "version": "0.0.1",
  "author": "redpelicans",
  "license": "MIT",
  "scripts": {
    "eslint": "eslint src",
    "client-dist": "webpack --mode=development --progress -w --colors",
    "client-watch": "npm run watch",
    "client-dev": "webpack-dev-server --colors --hot --inline --host 127.0.0.1 --port 8080",
    "srv-dev": "babel-watch -w src src/server/index.js",
    "srv-dist": "DEBUG=tetris:* babel src --out-dir dist",
    "test": "mocha --require babel-core/register --reporter spec",
    "coverage": "NODE_ENV=test nyc --include src --all -r text -r lcov -r text --require babel-core/register mocha "
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "babel-eslint": "^7.2.3",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "express": "^4.15.2",
    "file-loader": "^4.0.0",
    "fs": "0.0.1-security",
    "jest": "^26.3.0",
    "lodash": "^4.17.19",
    "net": "^1.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-functional": "^3.0.2",
    "react-key-handler": "^1.2.0-beta.3",
    "react-redux": "^4.4.5",
    "react-sound": "^1.2.0",
    "react-test-renderer": "^16.13.1",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "styled-components": "^5.1.1",
    "uifx": "^2.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/cli": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-__coverage__": "^0.111111.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-watch": "^7.0.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chai-equal-jsx": "^1.0.9",
    "css-loader": "^4.2.1",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^7.20.0",
    "mocha": "^2.4.5",
    "nyc": "^6.4.4",
    "style-loader": "^1.2.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.10.0"
  },
  "nyc": {
    "exclude": [
      "build"
    ]
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
